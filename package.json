{
	"name": "moonkake",
	"description": "Site starter template based on Vite, Scss, Pug, JS",
	"version": "10.2.0",
	"author": {
		"name": "DevBrains",
		"url": "https://devbrains.io"
	},
	"homepage": "https://github.com/pro-verstka/moonkake#readme",
	"repository": "git://github.com/pro-verstka/moonkake.git",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "pnpm run tools:lint && vite build",
		"tools:format": "biome format ./src --fix",
		"tools:lint": "biome check ./src --fix",
		"tools:generate": "plop"
	},
	"dependencies": {
		"fontfaceobserver": "2.3.0",
		"gsap": "3.12.5",
		"imask": "7.6.1",
		"swiper": "11.1.15"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@vituum/vite-plugin-pug": "1.1.0",
		"lightningcss": "1.28.2",
		"plop": "4.0.1",
		"sass-embedded": "1.83.0",
		"vite": "6.0.6",
		"vite-plugin-static-copy": "2.2.0"
	},
	"engines": {
		"node": ">=22.11.0"
	},
	"volta": {
		"node": "22.11.0"
	}
}
