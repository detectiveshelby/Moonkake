{
  "name": "moonkake",
  "version": "8.4.2",
  "description": "Start template. Supports Pug, Sass, PostCSS, Vue, React, ES6.",
  "author": "p1rate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/detectiveshelby/moonkake.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development gulp",
    "build": "cross-env NODE_ENV=production gulp build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": [
    "last 3 version",
    "ie >= 11"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "vue"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/plugin-transform-react-jsx",
        {
          "runtime": "automatic",
          "importSource": "react"
        }
      ]
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "useTabs": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "trailingComma": "none",
    "printWidth": 120
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.5",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/register": "^7.12.1",
    "autoprefixer": "^10.0.2",
    "babel-loader": "^8.2.1",
    "babel-preset-vue": "^2.0.2",
    "browser-sync": "^2.26.13",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-cheerio": "^1.0.0",
    "gulp-data": "^1.3.1",
    "gulp-flatten": "^0.4.0",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-notify": "^3.2.0",
    "gulp-postcss": "^9.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sass-glob": "^1.1.0",
    "node-sass": "^5.0.0",
    "node-sass-package-importer": "^5.3.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.0",
    "terser-webpack-plugin": "^5.0.3",
    "vue-loader": "^15.9.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.6.0",
    "webpack-stream": "^6.1.1"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-scroll-lock": "^3.1.5",
    "classlist-polyfill": "^1.2.0",
    "core-js": "^3.7.0",
    "custom-event-polyfill": "^1.0.7",
    "element-closest-polyfill": "^1.0.2",
    "flatpickr": "^4.6.6",
    "fontfaceobserver": "^2.1.0",
    "gsap": "^3.5.1",
    "imask": "^6.0.5",
    "nodelist-foreach-polyfill": "^1.2.0",
    "photoswipe": "^4.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "scroll-to-element": "^2.0.3",
    "scrollbooster": "^3.0.2",
    "scrollreveal": "^4.0.7",
    "swiped-events": "^1.1.4",
    "swiper": "^6.3.5",
    "tippy.js": "^6.2.7",
    "validate.js": "^0.13.1",
    "vanilla-lazyload": "^17.1.3",
    "vue": "^2.6.12"
  }
}
