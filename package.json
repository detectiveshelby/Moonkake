{
  "name": "moonkake",
  "version": "8.5.1",
  "description": "Start template. Supports Pug, Sass, PostCSS, Vue, React, ES6.",
  "author": {
    "name": "DevBrains",
    "url": "https://devbrains.io"
  },
  "license": "MIT",
  "repository": "git://github.com/detectiveshelby/moonkake.git",
  "scripts": {
    "dev": "cross-env NODE_ENV=development gulp",
    "build": "cross-env NODE_ENV=production gulp build"
  },
  "browserslist": [
    "last 3 version",
    "ie >= 11"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "vue"
    ],
    "plugins": [
      "@babel/plugin-proposal-private-methods",
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/plugin-transform-react-jsx",
        {
          "runtime": "automatic",
          "importSource": "react"
        }
      ]
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "useTabs": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "trailingComma": "none",
    "printWidth": 120
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "^2.0.2",
    "browser-sync": "^2.26.14",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "cssnano": "^5.0.2",
    "cssnano-preset-default": "^5.0.1",
    "del": "^6.0.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-cheerio": "^1.0.0",
    "gulp-data": "^1.3.1",
    "gulp-flatten": "^0.4.0",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-postcss": "^9.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "node-sass": "^5.0.0",
    "node-sass-package-importer": "^5.3.2",
    "postcss": "^8.2.14",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "terser-webpack-plugin": "^5.1.1",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.36.2",
    "webpack-stream": "^6.1.2"
  },
  "dependencies": {
    "body-scroll-lock": "^3.1.5",
    "classlist-polyfill": "^1.2.0",
    "core-js": "^3.11.1",
    "custom-event-polyfill": "^1.0.7",
    "element-closest-polyfill": "^1.0.2",
    "flatpickr": "^4.6.9",
    "fontfaceobserver": "^2.1.0",
    "imask": "^6.0.7",
    "nodelist-foreach-polyfill": "^1.2.0",
    "photoswipe": "^4.1.3",
    "scroll-to-element": "^2.0.3",
    "scrollbooster": "^3.0.2",
    "scrollreveal": "^4.0.9",
    "swiper": "^6.5.9",
    "validate.js": "^0.13.1",
    "vanilla-lazyload": "^17.3.1"
  }
}
