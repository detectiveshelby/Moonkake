{
	"name": "moonkake",
	"description": "Site starter template based on Vite, Scss, Pug, JS",
	"version": "10.0.14",
	"author": {
		"name": "DevBrains",
		"url": "https://devbrains.io"
	},
	"homepage": "https://github.com/pro-verstka/moonkake#readme",
	"repository": "git://github.com/pro-verstka/moonkake.git",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "pnpm run lint && vite build",
		"format": "pnpx @biomejs/biome format ./src --write",
		"lint": "pnpx @biomejs/biome lint ./src",
		"mk": "plop"
	},
	"dependencies": {
		"gsap": "3.12.5",
		"imask": "7.6.0",
		"swiper": "11.1.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.3",
		"@vituum/vite-plugin-pug": "1.1.0",
		"lightningcss": "1.25.0",
		"plop": "4.0.1",
		"sass": "1.77.2",
		"vite": "5.2.11"
	},
	"engines": {
		"node": ">=20.10.0"
	},
	"volta": {
		"node": "20.10.0"
	}
}
