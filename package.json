{
	"name": "moonkake",
	"description": "Site starter template based on Vite, Scss, Pug, JS",
	"version": "10.0.15",
	"author": {
		"name": "DevBrains",
		"url": "https://devbrains.io"
	},
	"homepage": "https://github.com/pro-verstka/moonkake#readme",
	"repository": "git://github.com/pro-verstka/moonkake.git",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "pnpm run tools:lint && vite build",
		"tools:format": "biome format ./src --fix --reporter=summary",
		"tools:lint": "biome check ./src --fix --reporter=summary",
		"tools:generate": "plop"
	},
	"dependencies": {
		"gsap": "3.12.5",
		"imask": "7.6.1",
		"swiper": "11.1.14"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.1",
		"@vituum/vite-plugin-pug": "1.1.0",
		"lightningcss": "1.27.0",
		"plop": "4.0.1",
		"sass": "1.79.1",
		"vite": "5.4.6",
		"vite-plugin-static-copy": "1.0.6"
	},
	"engines": {
		"node": ">=20.10.0"
	},
	"volta": {
		"node": "20.10.0"
	}
}
